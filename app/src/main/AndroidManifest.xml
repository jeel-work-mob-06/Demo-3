        android:supportsRtl="true"
        android:theme="@style/Theme.Demo3"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".NavigationActivity"
            android:exported="false"
            android:label="@string/title_activity_navigation"
            android:theme="@style/Theme.Demo3" />
        <activity
